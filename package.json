{
  "name": "flapjack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "dev:cmd": "cross-env LOCAL_COMMANDS=true nodemon",
    "dev:web": "cross-env WEB_ONLY=true nodemon",
    "dev:script": "nodemon --watch scripts --watch server --ext ts,yaml --exec ts-node --project tsconfig.server.json",
    "build": "rm -rf dist/ && next build && tsc --project tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "start": "cross-env NODE_ENV=development node dist/server/index.js",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "migration:make": "mikro-orm migration:create --blank --config ./server/config/db.ts",
    "migration:run": "mikro-orm migration:up --config ./server/config/db.ts"
  },
  "dependencies": {
    "@mikro-orm/core": "^6.4.16",
    "@mikro-orm/migrations": "^6.4.16",
    "@mikro-orm/mysql": "^6.4.16",
    "@napi-rs/canvas": "0.1.81",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "chroma-js": "^3.1.2",
    "cron-translate": "^1.0.0",
    "dayjs": "^1.11.13",
    "delta-e": "^0.0.8",
    "discord.js": "14.24.0",
    "engine-blackjack-ts": "^0.9.11",
    "express": "^4.18.2",
    "get-image-colors": "^4.0.1",
    "gif-encoder-2": "^1.0.5",
    "js-tiktoken": "^1.0.14",
    "mysql2": "^3.3.5",
    "next": "13.4.5",
    "next-absolute-url": "^1.2.2",
    "node-cron": "^3.0.3",
    "osrs-json-hiscores": "^2.20.0",
    "poe-api-manager": "^1.2.23",
    "pokersolver": "^2.1.4",
    "react": "18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "18.2.0",
    "react-string-replace": "^1.1.1",
    "react-use": "^17.6.0",
    "reflect-metadata": "0.2.1",
    "typescript": "5.1.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.4.16",
    "@svgr/webpack": "^8.1.0",
    "@types/delta-e": "^0.0.2",
    "@types/express": "^4.17.17",
    "@types/get-image-colors": "^4.0.5",
    "@types/jest": "^29.5.14",
    "@types/node-cron": "^3.0.11",
    "@types/react-color": "^3.0.13",
    "@types/styled-components": "5.1.34",
    "babel-plugin-styled-components": "2.1.4",
    "cross-env": "^7.0.3",
    "eslint": "9.27.0",
    "eslint-config-next": "^15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.5.3",
    "styled-components": "6.1.19",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript-eslint": "^8.32.1"
  }
}
